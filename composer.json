{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "*",
        "dompdf/dompdf": "*",
        "drupal/admin_toolbar": "^3.6",
        "drupal/antibot": "^2.0",
        "drupal/backup_migrate": "^5.0@RC",
        "drupal/better_search": "^1.5.0",
        "drupal/block_class": "^1.1.0",
        "drupal/block_token": "^1.2",
        "drupal/breadcrumb_extra_field": "^4.0",
        "drupal/captcha": "^1.1",
        "drupal/core": "*",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^3.2.0",
        "drupal/ds": "^3.6",
        "drupal/entity_print": "^2.1.0",
        "drupal/entity_reference_revisions": "^1.7.0",
        "drupal/extra_body_classes": "*",
        "drupal/feeds": "^3.0.0-alpha6",
        "drupal/image_class": "^2.0",
        "drupal/menu_breadcrumb": "^2.0",
        "drupal/metatag": "^2.1",
        "drupal/paragraphs": "^1.10.0",
        "drupal/pathauto": "^1.6.0",
        "drupal/quick_node_clone": "^1.12.0",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.5.0",
        "drupal/scanner": "^2",
        "drupal/search_api": "^1.15.0",
        "drupal/search_api_autocomplete": "^1.2.0",
        "drupal/simple_sitemap": "^4.2",
        "drupal/smtp": "^1.0.0-beta6",
        "drupal/token": "^1.15",
        "drupal/token_filter": "^2.2",
        "drupal/webform": "6.2.9",
        "drush/drush": "^12.5",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-feed": "^2.12",
        "laminas/laminas-stdlib": "^3.2",
        "psr/container": "*",
        "symfony/filesystem": "^6.4",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0"
    },
    "conflict": {
        "drupal/drupal": "*",
	    "drupal/material_base": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        }
    }
}
